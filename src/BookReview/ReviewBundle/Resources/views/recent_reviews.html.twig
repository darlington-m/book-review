
<div class="list-group" style="margin-top: 10px; margin-bottom: 10px">
    <a class="list-group-item active">
        Latest Reviews
    </a>
    {% for review in reviews %}
        <a href="{{ path('review_view', {'id': review.id}) }}" class="list-group-item">
            <span class="small">{{ review.title }}</span>
        </a>
    {% endfor %}
</div>
