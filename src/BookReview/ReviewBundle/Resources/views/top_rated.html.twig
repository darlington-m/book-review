<div class="list-group" style="margin-top: 10px; margin-bottom: 10px">
    <a class="list-group-item active">
        Top Rated Books
    </a>
    {% for book in books %}
        <a href="{{ path('book_view', {'id': book.id}) }}" class="list-group-item">
            <span class="small">{{ book.title }}</span>
        </a>
    {% endfor %}
</div>